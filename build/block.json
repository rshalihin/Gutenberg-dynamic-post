{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "demo-block/dynamic-block",
  "version": "0.1.0",
  "title": "Dynamic Block",
  "category": "text",
  "icon": "networking",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "dynamic-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "numberOfPages": {
      "type": "number",
      "default": 5
    },
    "displayFutureImage": {
      "type": "boolean",
      "default": true
    },
    "order": {
      "type": "string",
      "default": "asc"
    },
    "orderBy": {
      "type": "string",
      "default": "date"
    }
  }
}